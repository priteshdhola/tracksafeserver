<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
<%@ page session="false" %>

<fmt:setLocale value="en"/>
<fmt:setBundle basename="locale.messages" var="lang"/>


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">

    <title><fmt:message key="messages.common.header.projectName" bundle="${lang}"/></title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/segmentsDiscovery/resources/css/main.css">
</head>
<body class="neustar">
    <!-- Part 1: Wrap all page content here -->
    <div id="wrap">

    <!-- Header template -->
    <jsp:include page="/WEB-INF/pages/common/header.jsp" />
    <!-- End of Header template -->

    <!-- No script template -->
    <jsp:include page="/WEB-INF/pages/common/noscript.jsp" />
    <!-- End of No script template -->


        <!-- Begin page content -->
        <div id="ats-builder-content" class="container ats-builder-container">

            <div class="ats-builder-selector">

                <div class="row">

                    <!-- tabs left -->
                    <div class="tabbable tabs-left">
                        <ul id="builderTabs" class="nav nav-tabs" data-tabs="tabs">
                            <li class="active"><a href="#location" data-toggle="tab"><fmt:message key="messages.common.header.locationTab" bundle="${lang}"/></a></li>
                            <li><a href="#postalCodes" data-toggle="tab"><fmt:message key="messages.common.header.postalCodeTab" bundle="${lang}"/></a></li>
                            <li><a href="#organization" data-toggle="tab"><fmt:message key="messages.common.header.orgTab" bundle="${lang}"/></a></li>
                            <li><a href="#categories" data-toggle="tab"><fmt:message key="messages.common.header.catTab" bundle="${lang}"/></a></li>
                            <li><a href="#tags" data-toggle="tab"><fmt:message key="messages.common.header.tagTab" bundle="${lang}"/></a></li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane active" id="location">Location Tab Contents</div>
                            <div class="tab-pane" id="postalCodes">Postal Code Tab Contents</div>
                            <div class="tab-pane" id="organization">Organization Tab Contents</div>
                            <div class="tab-pane" id="categories">Categories Tab Contents</div>
                            <div class="tab-pane" id="tags">Tag Tab Contents</div>
                        </div>
                    </div>
                    <!-- /tabs -->
                </div>

            </div>

        </div>


    </div> <!-- End of Wrap -->


    <!-- Footer template -->
    <jsp:include page="/WEB-INF/pages/common/footer.jsp" />
    <!-- End of Footer template -->


    <script data-main="/segmentsDiscovery/resources/js/main"
            src="/segmentsDiscovery/resources/js/libs/require/require.js"></script>

</body>
</html>
